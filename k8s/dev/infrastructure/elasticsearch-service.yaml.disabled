apiVersion: v1
kind: Service
metadata:
  name: elasticsearch
  namespace: suneasy-dev
  labels:
    app: elasticsearch
    tier: infrastructure
spec:
  type: ClusterIP
  ports:
  - port: 9200
    targetPort: 9200
    protocol: TCP
    name: http
  - port: 9300
    targetPort: 9300
    protocol: TCP
    name: transport
  selector:
    app: elasticsearch
